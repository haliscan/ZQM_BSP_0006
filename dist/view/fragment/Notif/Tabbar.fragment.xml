<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:table="sap.ui.table"
>
    <IconTabBar
        id="IdTabBar"
        expandable="false"
        expanded="true"
    >
        <items>
            <IconTabFilter
                text="{i18n>ApprovementTab}"
                key="Approvement"
                id="IdTabApprovement"
            >
                <content>
                    <core:Fragment
                        fragmentName="com.karsan.qm.taskmanage.view.fragment.Notif.Tabbar.Approvement"
                    />
                </content>
            </IconTabFilter>
            <IconTabFilter
                text="{i18n>NotifDetailTab}"
                key="NotifDetail"
                id="IdTabNotifDetail"
            >
                <content>
                    <core:Fragment
                        fragmentName="com.karsan.qm.taskmanage.view.fragment.Notif.Tabbar.NotifDetail"
                    />
                </content>
            </IconTabFilter>
        </items>
    </IconTabBar>
</core:FragmentDefinition>


