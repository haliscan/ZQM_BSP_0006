                            <smartFilterBar:SelectOption
                                sign="I"
                                operator="EQ"
                                low="Y1"
                            />
                        </smartFilterBar:defaultFilterValues>
                    </smartFilterBar:ControlConfiguration>

                    <smartFilterBar:ControlConfiguration
                        key="Check48"
                        filterType="single"
                        label="{i18n>Check48}"
                        controlType="dropDownList"
                        displayBehaviour="idAndDescription"
                    />

                    <smartFilterBar:ControlConfiguration
                        key="ErdatTsk"
                        filterType="interval"
                        controlType="date"
                    />

                    <smartFilterBar:ControlConfiguration
                        key="NtfStat"
                        filterType="single"
                        label="{i18n>NtfStat}"
                        controlType="dropDownList"
                        displayBehaviour="idAndDescription"
                    />
                </smartFilterBar:controlConfiguration>
